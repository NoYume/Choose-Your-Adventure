{
  "version": 2,
  "functions": {
    "main.py": {
      "runtime": "vercel-python@3.12"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/docs",
      "dest": "/main.py"
    },
    {
      "src": "/redoc", 
      "dest": "/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ]
}
